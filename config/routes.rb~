Rails.application.routes.draw do
  resources :projects
 
  get '/objectives/new',        to: 'objectives#new',     as: :new_objective
#  get '/objectives/destroy/:id' => 'objectives#destroy',  :destroy_objective
  get '/results/new',           to: 'results#new',        as: :new_result
#  get '/results/destroy/:id'    => 'results#destroy',     :destroy_result
  get '/indicators/new',        to: 'indicators#new',      as: :new_indicator
#  get '/indicators/destroy/:id' => 'indicators#destroy',  :destroy_indicator
#
  root "projects#index"
end
